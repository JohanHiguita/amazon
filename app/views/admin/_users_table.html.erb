<h3>Users</h3>
<div class="table-responsive">
	<table class="table table-striped table-sm">
		<thead>
			<tr>
				<th>id</th>
				<th>email</th>
				<th>Role</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<th><%= user.id %></th>
				<td><%= user.email%></td>
				<td><%= user.role %></td>
			</tr>

			<% end %>

			
			
		</tbody>
	</table>
</div><br>